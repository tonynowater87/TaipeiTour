<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Cathaytest" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Text.TitleLarge" parent="TextAppearance.Material3.TitleLarge" />
    <style name="Theme.Text.TitleMedium" parent="TextAppearance.Material3.TitleMedium" />
    <style name="Theme.Text.TitleSmall" parent="TextAppearance.Material3.TitleSmall" />
    <style name="Theme.Text.BodyLarge" parent="TextAppearance.Material3.BodyLarge" />
    <style name="Theme.Text.BodyMedium" parent="TextAppearance.Material3.BodyMedium" />
    <style name="Theme.Text.BodySmall" parent="TextAppearance.Material3.BodySmall" />

</resources>